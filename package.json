{
  "name": "@arthurka/ts-utils",
  "version": "1.2.0",
  "description": "Some convenient TS utilities.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [],
  "scripts": {
    "build:clean": "rm -rf build/*",
    "ts": "tsc -w",
    "ts-compile": "tsc --declaration",
    "my-compile": "node ../compile.js",
    "build": "lock-verify && npm run build:clean && npm run ts-compile && npm run my-compile",
    "publish:prepare": "mkdir -p ./build/build && mv ./build/* ./build/build; cp ./package* README.md ./build",
    "pu": "npm pu ./build",
    "my-publish": "npm run publish:prepare && npm run pu"
  },
  "dependencies": {},
  "devDependencies": {
    "@arthurka/eslint": "^1.6.0",
    "lock-verify": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurKa/ts-utils.git"
  },
  "author": {
    "name": "ArthurKa",
    "email": "arthur.katruk@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArthurKa/ts-utils/issues"
  },
  "homepage": "https://github.com/ArthurKa/ts-utils#readme"
}
